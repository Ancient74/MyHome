<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyHomeApp.ToggleButtonCapabilityView">
  <ContentView.Content>
        <StackLayout>
            <Switch IsEnabled="{Binding IsEnabled}" IsToggled="{Binding IsToggled}"/>
        </StackLayout>
    </ContentView.Content>
</ContentView>
